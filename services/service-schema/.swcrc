{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2019",
    "externalHelpers": true
  },
  "module": {
    "type": "es6",
    "strict": true
  },
  "sourceMaps": true
}
